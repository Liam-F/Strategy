Public Sub MergeInTechnicalTickers()    Dim i, j, ouputLine, readFromCol As Integer    Dim newTicker As String        readFromCol = 4        i = 3    j = 3    outputLine = 3        While Cells(outputLine, 2) <> ""        outputLine = outputLine + 1    Wend        While Cells(i, readFromCol).Value <> ""        newTicker = Cells(i, readFromCol).Value                newTicker = Replace(newTicker, " UN ", " US ")        newTicker = Replace(newTicker, " UW ", " US ")                j = 3        While Cells(j, 2).Value <> newTicker And Cells(j, 2) <> ""            j = j + 1        Wend                If (Cells(j, 2).Value = "") Then 'Match not found            Cells(outputLine, 2).Value = newTicker            outputLine = outputLine + 1        End If                i = i + 1    Wend    End SubPublic Sub GetTickersFromFileNames()    Dim ticker As String    Dim i, readFromCol As Integer    i = 3    readFromCol = 4        Dim fs, f As Variant    Set fs = CreateObject("Scripting.FileSystemObject")    Set f = fs.GetFolder("S:\optprice\Equity Derivs\Trading\Volatility\VolsHistory")        For Each myFile In f.Files        ticker = myFile.name        ticker = Split(ticker, ".")(0)        ticker = ticker & " US Equity"                Cells(i, readFromCol).Value = ticker        i = i + 1    Next    End Sub
